                            },
                            "APPC_LCM_RESPONSE": {
                                "adaptedClass": "org.onap.policy.appclcm.LcmResponse",
                                "adaptorClass": "org.onap.policy.appclcm.util.Serialization$ResponseAdapter"
                            }
                        }
                    }
                }
            }
        }
    },
    "eventOutputParameters": {
